  <profiles>
		<profile>
			<id>sonar</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<sonar.jdbc.url>
					jdbc:mysql://192.168.1.60:3306/sonar_db?useUnicode=true&amp;characterEncoding=utf8
